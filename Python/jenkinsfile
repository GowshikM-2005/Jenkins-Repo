pipeline {
    agent any

    stages {
        stage('CheckOut') {
            steps {
               checkout scmGit(branches: [[name: '*/dev']], extensions: [], userRemoteConfigs: [[credentialsId: 'ac7e6773-d0b7-48f7-b4ec-657c450d43cc', url: 'https://github.com/GowshikM-2005/Jenkins-Repo.git']])
            }
        }
        stage('Build'){
            steps{
            git branch: 'dev', credentialsId: 'ac7e6773-d0b7-48f7-b4ec-657c450d43cc', url: 'https://github.com/GowshikM-2005/Jenkins-Repo.git'
            bat'python sort.py'
            }
        }
        stage('Test'){
            steps{
                echo 'Build Completed'
            }
        }
    }
}
